			<zone xml:id="facs_306_r36" ulx="327" uly="2067" lrx="411" lry="2092" rendition="Line" subtype="paragraph"/>
			<zone xml:id="facs_306_r35" ulx="327" uly="2026" lrx="1439" lry="2056" rendition="Line" subtype="paragraph"/>
			<zone xml:id="facs_306_r26" ulx="262" uly="1621" lrx="415" lry="1646" rendition="Line" subtype="paragraph"/>
