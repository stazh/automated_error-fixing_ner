			<zone xml:id="facs_306_r15" ulx="752" uly="1027" lrx="1276" lry="1056" rendition="Line" subtype="paragraph"/>
			<zone xml:id="facs_306_r33" ulx="277" uly="1976" lrx="319" lry="2002" rendition="Line" subtype="paragraph"/>
