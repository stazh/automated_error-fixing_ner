			<zone xml:id="facs_444_r27" ulx="1141" uly="1331" lrx="1440" lry="1356" rendition="Line" subtype="paragraph"/>
			<zone xml:id="facs_444_r24" ulx="329" uly="1293" lrx="525" lry="1316" rendition="Line" subtype="paragraph"/>
