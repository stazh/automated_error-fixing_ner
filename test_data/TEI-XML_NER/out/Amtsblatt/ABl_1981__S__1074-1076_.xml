			<zone xml:id="facs_88_r0" ulx="278" uly="143" lrx="350" lry="163" rendition="Line" subtype="paragraph"/>
			<zone xml:id="facs_88_r18" ulx="274" uly="912" lrx="1449" lry="945" rendition="Line" subtype="paragraph"/>
			<zone xml:id="facs_88_r3" ulx="279" uly="322" lrx="1457" lry="352" rendition="Line" subtype="paragraph"/>
			<zone xml:id="facs_88_r31" ulx="1222" uly="1479" lrx="1457" lry="1507" rendition="Line" subtype="paragraph"/>
