			<zone xml:id="facs_127_r8" ulx="256" uly="439" lrx="373" lry="465" rendition="Line" subtype="paragraph"/>
			<zone xml:id="facs_127_r38" ulx="627" uly="1759" lrx="1112" lry="1787" rendition="Line" subtype="paragraph"/>
			<zone xml:id="facs_127_r36" ulx="250" uly="1667" lrx="443" lry="1694" rendition="Line" subtype="paragraph"/>
			<zone xml:id="facs_127_r3" ulx="312" uly="258" lrx="1427" lry="288" rendition="Line" subtype="paragraph"/>
			<zone xml:id="facs_127_r25" ulx="306" uly="1144" lrx="1415" lry="1175" rendition="Line" subtype="paragraph"/>
